// c function to insert before key 
typedef struct Node
{
int data;
struct Node*next;
}Node;

Node* head = NULL;

void insert_before_key (Node**head,int data,int key)
{
if (*head==NULL)
{
printf ("no nodes\n");
return;
}
Node*new_node = (Node*) malloc (sizeof(int));
new_node->data =data;
new_node->next = NULL;

Node*prev = *head;
Node*cur = (*head)->next;
while (cur!=NULL && cur->data !=key)
{
prev = cur;
cur = cur->next;
}
